{
  "name": "resium",
  "version": "1.2.1",
  "description": "React components for Cesium",
  "main": "dist/resium.cjs.js",
  "module": "dist/resium.es.js",
  "types": "dist/types/src/index.d.ts",
  "repository": "https://github.com/darwin-education/resium.git",
  "author": "rot1024 <aayhrot@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest --coverage",
    "clean": "del dist coverage examples/build",
    "clean:build": "del dist",
    "lint": "run-p lint:js lint:ts",
    "lint:js": "eslint .",
    "lint:ts": "tslint 'src/**/*.ts?(x)'",
    "build": "run-s clean:build build:es build:cjs build:umd build:umd:min",
    "build:es": "cross-env NODE_ENV=es rollup -c",
    "build:cjs": "cross-env NODE_ENV=cjs rollup -c",
    "build:umd": "rollup -c",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c",
    "format": "run-p format:js format:ts",
    "format:js": "prettier-eslint --write '*.js' --write 'src/**/*.js' --write '.storybook/**/*.js'",
    "format:ts": "prettier --write '**/*.ts' --write '**/*.tsx'",
    "storybook": "start-storybook -p 9001",
    "doc": "node docs/scripts/generateDoc && docz dev",
    "doc:build": "node docs/scripts/generateDoc && docz build",
    "doc:generate": "node docs/scripts/generateDoc"
  },
  "files": [
    "src",
    "dist"
  ],
  "engines": {
    "node": ">=8.3.0"
  },
  "peerDependencies": {
    "cesium": "1.x",
    "react": ">=16.3",
    "react-dom": ">=16.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@emotion/cache": "^10.0.0",
    "@emotion/core": "^10.0.6",
    "@emotion/styled": "^10.0.6",
    "@storybook/addon-actions": "^4.1.7",
    "@storybook/react": "^4.1.7",
    "@types/cesium": "^1.47.3",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.13",
    "@types/lodash.omit": "^4.5.4",
    "@types/lodash.pick": "^4.4.4",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__react": "^4.0.0",
    "babel-loader": "^8.0.5",
    "cesium": "^1.53.0",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "del-cli": "^1.1.0",
    "docz": "^0.13.7",
    "docz-theme-default": "^0.13.7",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-node": "^8.0.1",
    "file-loader": "^3.0.1",
    "jasmine-core": "^3.3.0",
    "jest": "^23.6.0",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.0",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "rollup": "^1.1.2",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^4.0.2",
    "rollup-plugin-typescript2": "^0.19.2",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0"
  }
}
