{
  "name": "bible2",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --mode development --progress",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "build": "webpack --mode production",
    "clean": "del build coverage",
    "format": "run-p format:js format:ts format:css format:json format:md",
    "format:js": "prettier-eslint --write '**/*.js' --write '**/*.jsx'",
    "format:ts": "prettier --write '**/*.ts' --write '**/*.tsx'",
    "format:css": "prettier-stylelint --write '**/*.css'",
    "format:json": "prettier --write **/*.json",
    "format:md": "prettier --write **/*.md",
    "lint": "run-p lint:js lint:ts lint:css",
    "lint:js": "eslint .",
    "lint:ts": "tslint 'src/**/*.ts?(x)'",
    "lint:css": "stylelint 'src/**/*.css'",
    "storybook": "start-storybook -p 9001",
    "storybook:build": "build-storybook -o storybook"
  },
  "engines": {
    "node": ">=8"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "enableTsDiagnostics": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.ts",
      "\\.css$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/__mocks__/setup.ts"
    ],
    "testRegex": ".*\\.(test|spec)\\.(tsx?)$",
    "transform": {
      "\\.tsx?$": "ts-jest"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.42",
    "@storybook/react": "^3.4.11",
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "^3.1.9",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^22.2.2",
    "@types/node": "^9.6.0",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.40",
    "@types/react-dom": "^16.0.4",
    "@types/react-hot-loader": "^3.0.6",
    "@types/react-redux": "^5.0.15",
    "@types/react-router-dom": "^4.2.5",
    "@types/recompose": "^0.24.6",
    "@types/storybook__react": "^3.0.7",
    "@types/webpack-env": "^1.13.5",
    "babel-loader": "^7.1.4",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "del-cli": "^1.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "hard-source-webpack-plugin": "^0.6.4",
    "html-webpack-include-assets-plugin": "^1.0.4",
    "html-webpack-plugin": "^3.1.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.0",
    "npm-run-all": "^4.1.2",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.3",
    "postcss-smart-import": "^0.7.6",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1",
    "prettier-stylelint": "^0.4.2",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.2.0",
    "style-loader": "^0.20.3",
    "stylelint": "^9.1.3",
    "stylelint-config-css-modules": "^1.2.0",
    "stylelint-config-idiomatic-order": "^5.0.0",
    "stylelint-config-standard": "^18.2.0",
    "thread-loader": "^1.1.5",
    "ts-jest": "^22.4.2",
    "ts-loader": "^4.1.0",
    "ts-loader-v3": "npm:ts-loader@^3.5.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.5.1",
    "typescript": "^3.3.3",
    "webpack": "^4.2.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "@storybook/addon-actions": "^4.1.11",
    "@types/cesium": "^1.47.3",
    "cesium": "^1.54.0",
    "cesium-react": "^0.2.1",
    "classnames": "^2.2.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0",
    "resium": "^1.2.1"
  }
}
